pan class="line">   wg.Wait()</span><br><span class="line">&#125;</span><br><span class="line"></span><br><span class="line"><span class="function"><span class="keyword">func</span> <span class="title">output</span><span class="params">(v <span class="type">int</span>)</span></span> &#123;</span><br><span class="line">   time.Sleep(time.Duration(v) * time.Second)</span><br><span class="line">   fmt.Println(v)</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>]]></content>
    
    
      
      
